{
    "name": "backend",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "set DEBUG=backend:* & npm start",
        "babel-node": "babel-node --presets=latest",
        "start": "nodemon -r dotenv/config --exec babel-node ./src/bin/www",
        "clean": "rimraf package-lock.json",
        "restart": "rimraf dist && npm run build && npm run start",
        "build": "npm run clean && babel src --out-dir dist --source-maps inline --copy-files"
    },
    "dependencies": {
        "@hapi/joi": "^17.1.0",
        "babel-node": "0.0.1-security",
        "body-parser": "^1.19.0",
        "connect-history-api-fallback": "^1.6.0",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.9.3",
        "morgan": "~1.9.1",
        "multer": "^1.4.2",
        "nodemon": "^2.0.2",
        "pug": "2.0.0-beta11",
        "sharp": "^0.25.2",
        "uuid": "^7.0.1",
        "vhost": "^3.0.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/node": "^7.8.4",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/preset-env": "^7.8.6",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^8.2.6",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-prettier": "^3.1.2",
        "rimraf": "^3.0.2"
    },
    "engines": {
        "node": "12.16.1",
        "npm": "6.13.4"
    }
}
